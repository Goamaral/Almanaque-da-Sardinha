<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th>Admin</th>
      <th>Moderator</th>
    </tr>
  </thead>

  <tbody>
    <% users.forEach(user => { %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%= user.is_admin %></td>
      <td><%= user.is_moderator %></td>
      <td><a href="#">Show</a></td>
      <td><a href="#">Edit</a></td>
      <td><a href="#">Delete</a></td>
    </tr>
    <% }) %>
  </tbody>
</table>